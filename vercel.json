{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "maseno-counseling-bot-v2/dashboard/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "maseno-counseling-bot-v2/dashboard/dist" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "maseno-counseling-bot-v2/dashboard/index.html"
    }
  ],
  "functions": {
    "backend/src/index.js": {
      "maxDuration": 30
    }
  }
}
