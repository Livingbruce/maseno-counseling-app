{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/dist/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/dashboard/dist/index.html" }
  ]
}