{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/test-simple.js": {
      "maxDuration": 10
    },
    "api/login-simple.js": {
      "maxDuration": 10
    },
    "api/login-working.js": {
      "maxDuration": 10
    },
    "api/auth/login.js": {
      "maxDuration": 10
    },
    "api/auth/me.js": {
      "maxDuration": 10
    }
  }
}
